    function llenarFinPlan() {
        var ff = document.getElementById('accion_control_fechaInicioPlan');
        if(ff.value!=='' && ff.value!==null){
            var d = new Date(ff.value);
            d.setDate(d.getDate() + 14);
            var m = d.getMonth() + 1;
            var day = d.getDate() + 1;
            if(m >= 1 && m <= 9){
                m = "0" + m;
            }
            if(day >= 1 && day <= 9){
                day = "0" + day;
            }
            var resp = d.getFullYear() + "-" + m + "-" + day;
            //alert(resp);
            document.getElementById("accion_control_fechaFinPlan").value = resp.toString();
        }
    }
