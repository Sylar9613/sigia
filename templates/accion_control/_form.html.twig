<script>
    function validateFecha(fecha) {
        if (fecha === '1'){
            var ffp = document.getElementById("accion_control_fechaFinPlan");
            if(ffp.value!=='' && ffp.value!==null){
                var fip = document.getElementById("accion_control_fechaInicioPlan");
                if(fip.value!=='' && fip.value!==null){
                    var end = new Date(ffp.value);
                    var start = new Date(fip.value);
                    if(end <= start){
                        var m = start.getMonth() + 1;
                        var day = start.getDate() + 1;
                        if(m >= 1 && m <= 9){
                            m = "0" + m;
                        }
                        if(day >= 1 && day <= 9){
                            day = "0" + day;
                        }
                        var resp = day + "-" + m + "-" + start.getFullYear();
                        $('#req-ffp').removeClass('collapse');
                        document.getElementById("span-ffp").innerHTML = "La fecha final del plan debe ser mayor que la fecha inicial (" + resp.toString() + ").";
                        $('#accion_control_fechaFinPlan').css("border", "1px solid #dc3545");
                        $('#accion_control_fechaFinPlan:invalid').css("border", "1px solid #dc3545");
                    }
                    else {
                        $('#req-ffp').addClass('collapse');
                        $('#accion_control_fechaFinPlan').css("border", "1px solid #808080");
                    }
                }
                else {
                    $('#req-ffp').removeClass('collapse');
                    document.getElementById("span-ffp").innerHTML = "Debe definir primero la fecha de inicio del plan.";
                    $('#accion_control_fechaFinPlan').css("border", "1px solid #dc3545");
                    $('#accion_control_fechaFinPlan:invalid').css("border", "1px solid #dc3545");
                }
            }
        }
        if (fecha === '2'){
            var ffr = document.getElementById("accion_control_fechaFinReal");
            if(ffr.value!=='' && ffr.value!==null){
                var fir = document.getElementById("accion_control_fechaInicioReal");
                if(fir.value!=='' && fir.value!==null){
                    var end2 = new Date(ffr.value);
                    var start2 = new Date(fir.value);
                    if(end2 <= start2){
                        var m2 = start2.getMonth() + 1;
                        var day2 = start2.getDate() + 1;
                        if(m2 >= 1 && m2 <= 9){
                            m2 = "0" + m2;
                        }
                        if(day2 >= 1 && day2 <= 9){
                            day2 = "0" + day2;
                        }
                        var resp2 = day2 + "-" + m2 + "-" + start2.getFullYear();
                        $('#req-ffr').removeClass('collapse');
                        document.getElementById("span-ffr").innerHTML = "La fecha final real debe ser mayor que la fecha inicial (" + resp2.toString() + ").";
                        $('#accion_control_fechaFinReal').css("border", "1px solid #dc3545");
                        $('#accion_control_fechaFinReal:invalid').css("border", "1px solid #dc3545");
                    }
                    else {
                        $('#req-ffr').addClass('collapse');
                        $('#accion_control_fechaFinReal').css("border", "1px solid #808080");
                    }
                }
                else {
                    $('#req-ffr').removeClass('collapse');
                    document.getElementById("span-ffr").innerHTML = "Debe definir primero la fecha de inicio real.";
                    $('#accion_control_fechaFinReal').css("border", "1px solid #dc3545");
                    $('#accion_control_fechaFinReal:invalid').css("border", "1px solid #dc3545");
                }
            }
        }
    }
